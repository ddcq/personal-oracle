{
  "general": {
    "checkpointing": {
      "enabled": true
    }
  },
  "mcpServers": {
    "dart": {
      "command": "dart",
      "args": [
        "mcp-server"
      ]
    },
    "github": {
        "httpUrl": "https://api.githubcopilot.com/mcp/",
        "headers": {
            "Authorization": "Bearer $GITHUB_MCP_PAT"
        }
    },
    "sequential-thinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    }
  }
}
